{
    "name": "matks/customer-support-bundle",
    "type": "symfony-bundle",
    "description": "Tickets-based bundle to integrate customer exchange in your symfony2 application",
    "homepage": "https://github.com/matks/CustomerSupportBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Mathieu Ferment",
            "email": "mathieu.ferment@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "Matks\\Bundle\\CustomerSupportBundle": "" }
    },
    "target-dir": "Matks/Bundle/CustomerSupportBundle",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.4",
        "symfony/symfony": "2.3.*",
        "symfony/framework-bundle": "2.3.*",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "doctrine/doctrine-bundle": "1.2.*",
        "knplabs/doctrine-behaviors": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*@dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}