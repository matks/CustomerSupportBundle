{
    "name": "matks/customer-support-bundle",
    "type": "symfony-bundle",
    "description": "Tickets-based bundle to integrate customer exchange in your symfony2 application",
    "homepage": "https://github.com/matks/CustomerSupportBundle",
    "license": "MIT",
    "autoload": {
        "psr-0": { "Matks\\Bundle\\CustomerSupportBundle": "" }
    },
    "target-dir": "Matks/Bundle/CustomerSupportBundle",
    "prefer-stable": true,
    "require": {
        "symfony/symfony": "^4.0",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-bundle": "^1.8",
        "knplabs/doctrine-behaviors": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "behat/symfony2-extension": "^2.1",
        "codedungeon/phpunit-result-printer": "^0.4.4"
    }
}
